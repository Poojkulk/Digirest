<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name = "com.digirest.bean.UsersBean" table = "users">
      
      <meta attribute = "class-description">
         This class contains the User detail. 
      </meta>
      
      <id name = "id" type = "int" column = "id">
         <generator class="native"/>
      </id>
      
      <property name = "account_non_expired" column = "account_non_expired" type = "int"/>
       <property name = "account_non_locked" column = "account_non_locked" type = "int"/>
      <property name = "credentials_non_expired" column = "credentials_non_expired" type = "int"/>
      <property name = "enabled" column = "enabled" type = "int"/>
         <property name = "password" column = "password" type = "string"/>
      <property name = "username" column = "username" type = "string"/>
      
      <property name = "profile_id" column = "profile_id" type = "int"/>
      
         
      
      
      
      
      
   </class>
</hibernate-mapping>